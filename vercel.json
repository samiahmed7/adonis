{
  "buildCommand": "node ace build --production",
  "devCommand": "node ace serve --watch",
  "installCommand": "npm ci --production",
  "outputDirectory": "build",
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
